{
	"name": "montavistamun",
	"description": "Monta Vista Model United Nations Club Website",
	"license": "MIT",
	"author": "Jeffrey Meng <jeffkmeng@gmail.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/jeffreymeng/montavistamun"
	},
	"bugs": {
		"url": "https://github.com/jeffreymeng/montavistamun/issues"
	},
	"version": "2.0.0",
	"scripts": {
		"analyze": "npm run analyze:lint && npm run analyze:prettier",
		"analyze:lint": "eslint --ext .jsx --ext .js .",
		"analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
		"build": "gatsby build && npm run functions:build",
		"clean": "gatsby clean",
		"dev": "npm run develop",
		"develop": "gatsby develop -H 0.0.0.0",
		"fix": "npm run fix:lint && npm run fix:prettier",
		"fix:lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx . --fix",
		"fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
		"functions:install": "netlify-lambda install src/netlify-functions",
		"functions:build": "netlify-lambda build src/netlify-functions",
		"serve": "gatsby serve",
		"start": "npm run develop",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
	},
	"dependencies": {
		"@tailwindcss/custom-forms": "0.2.1",
		"@tailwindcss/ui": "^0.4.0",
		"@types/classnames": "^2.2.10",
		"@types/react": "^16.9.43",
		"@types/react-body-classname": "^1.1.6",
		"@types/react-dom": "^16.9.8",
		"@types/react-helmet": "^6.0.0",
		"@types/react-transition-group": "^4.4.0",
		"@typescript-eslint/eslint-plugin": "^3.7.0",
		"@typescript-eslint/parser": "^3.7.0",
		"autoprefixer": "9.8.5",
		"axios": "^0.19.2",
		"babel-eslint": "10.1.0",
		"classnames": "^2.2.6",
		"cssnano": "4.1.10",
		"eslint": "7.5.0",
		"eslint-loader": "4.0.2",
		"eslint-plugin-import": "2.22.0",
		"eslint-plugin-react": "7.20.3",
		"gatsby": "2.24.9",
		"gatsby-image": "^2.4.13",
		"gatsby-plugin-eslint": "2.0.8",
		"gatsby-plugin-manifest": "2.4.20",
		"gatsby-plugin-offline": "3.2.20",
		"gatsby-plugin-postcss": "^2.3.11",
		"gatsby-plugin-react-helmet": "3.3.10",
		"gatsby-plugin-sharp": "^2.6.21",
		"gatsby-source-filesystem": "^2.3.22",
		"gatsby-transformer-sharp": "^2.5.11",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.11",
		"netlify-lambda": "^2.0.1",
		"postcss-import": "^12.0.1",
		"prettier": "^2.0.5",
		"prop-types": "15.7.2",
		"react": "16.13.1",
		"react-body-classname": "^1.3.1",
		"react-dom": "16.13.1",
		"react-helmet": "6.1.0",
		"react-transition-group": "^4.4.1",
		"tailwindcss": "1.5.2",
		"typeface-inter": "^3.12.0",
		"typescript": "^3.9.7"
	},
	"keywords": [
		"gatsby",
		"purgecss",
		"tailwind",
		"tailwindcss"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,ts,jsx,tsx}": "eslint",
		"*.{ts,tsx,js,jsx,json,md,scss,css}": "prettier --write"
	}
}
